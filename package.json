{
  "name": "no9-webapp",
  "version": "0.1.1",
  "description": "webapp for no9",
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-build",
    "start": "webpack-dashboard -- babel-node tools/srcServer.js",
    "clean-build": "npm run remove-build && mkdir build",
    "remove-build": "rimraf ./build",
    "build:html": "babel-node tools/buildHtml.js",
    "build:server": "babel-node tools/build.js --server",
    "prebuild": "npm run clean-build",
    "build": "NODE_ENV=production webpack --progress --hide-modules --config tools/webpack.config.prod.babel.js",
    "postbuild": "rimraf ./dist && mv build dist",
    "prettier": "prettier  --write '{src,tools}/**/*.{js,jsx,json}'",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json}": ["prettier --write", "git add"]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": true
  },
  "author": "Qi Liu",
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "^6.26.0",
    "compression": "1.7.2",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "express": "4.16.3",
    "helmet": "3.12.1",
    "history": "^4.7.2",
    "id-validator": "^1.3.0",
    "immutable": "3.8.2",
    "lodash": "4.17.10",
    "mitt": "1.1.3",
    "morgan": "^1.8.2",
    "pingpp-js": "^2.1.14",
    "prop-types": "15.6.1",
    "pug": "2.0.3",
    "qrcode.react": "^0.7.1",
    "qs": "6.5.2",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "react-fastclick": "^3.0.2",
    "react-helmet": "^5.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.15.4",
    "react-waypoint": "^7.1.0",
    "recharts": "^1.0.0-apha.5",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-form": "6.8",
    "redux-immutable": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-saga": "0.16.0",
    "reselect": "^3.0.1",
    "serve-favicon": "2.5.0",
    "styled-components": "3.2.6",
    "validator": "9.1.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-recharts": "1.1.1",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "2.24.4",
    "chalk": "2.4.1",
    "co": "^4.6.0",
    "connect-history-api-fallback": "1.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "file-loader": "1.1.11",
    "glob": "^7.1.2",
    "happypack": "5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "7.1.1",
    "npm-run-all": "4.1.3",
    "prettier": "1.12.1",
    "pug-loader": "2.4.0",
    "react-hot-loader": "3.1.2",
    "rimraf": "^2.6.0",
    "svg-react-loader": "^0.4.5",
    "svgo": "1.0.5",
    "svgo-loader": "2.1.0",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "2.12.0",
    "webpack-dashboard": "1.1.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "2.22.2"
  }
}
